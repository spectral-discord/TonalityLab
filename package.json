{
  "scripts": {
    "migrate": "yarn rw prisma migrate dev"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@axe-core/react": "^4.7.1",
    "@redwoodjs/core": "5.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier-plugin-tailwindcss": "^0.2.8"
  },
  "eslintConfig": {
    "extends": [
      "@redwoodjs/eslint-config",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "root": true
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.1"
}
